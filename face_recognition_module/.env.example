# Supabase Configuration for Face Recognition Module
# Copy this to .env file and fill in your Supabase credentials

# ==================== SUPABASE CONFIGURATION ====================

# Your Supabase Project URL (found in Project Settings -> API)
# Example: https://your-project-id.supabase.co
SUPABASE_URL=your_supabase_url_here

# Supabase Anonymous Key (found in Project Settings -> API)
# This key is safe to use in client-side applications
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key (found in Project Settings -> API)
# This key should be kept secret and used only in server-side applications
# Comment out if you don't want to use service role
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# ==================== OPTIONAL CONFIGURATION ====================

# Database connection timeout (seconds)
DATABASE_TIMEOUT=30

# Maximum retry attempts for database operations
MAX_DB_RETRIES=3

# Cache duration for person data (seconds)
PERSON_CACHE_DURATION=300

# ==================== FACE RECOGNITION SETTINGS ====================

# Face recognition similarity threshold (0.0 to 1.0)
SIMILARITY_THRESHOLD=0.4

# GPU memory limit for RTX 3050 (MB)
GPU_MEMORY_LIMIT=2048

# Maximum faces to process per image
MAX_FACES_PER_IMAGE=20

# ==================== LOGGING CONFIGURATION ====================

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Log file path (optional)
LOG_FILE_PATH=face_recognition.log

# Enable performance tracking
PERFORMANCE_TRACKING=true

# ==================== SECURITY SETTINGS ====================

# Enable authentication for API endpoints
REQUIRE_AUTHENTICATION=false

# API rate limit per minute
RATE_LIMIT_PER_MINUTE=100

# Maximum concurrent requests
MAX_CONCURRENT_REQUESTS=10